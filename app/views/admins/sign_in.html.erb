<h1>Sign In</h1>

<%= form_tag admins_path do %>
  <p>
    <%= label_tag :email %><br />
    <%= text_field_tag :email %>
  </p>
  <p>
    <%= label_tag :password %><br />
    <%= password_field_tag :password %>
  </p>

  <p><%= submit_tag "Sign In", :class => "btn btn-primary" %></p>

<% end %>



